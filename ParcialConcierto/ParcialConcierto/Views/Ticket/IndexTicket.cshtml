@model ParcialConcierto.Models.TicketViewModel
@{
    ViewData["Title"] = "IndexTicket";
}

<div class="text-center">
    <h1 class="display-4">Consultar ticket</h1>
    <form asp-action="SearchTicket">
        <div class="form-group">
            <label asp-for="Id" class="control-label"></label>
            <input asp-for="Id" class="form-control" />
            <span asp-validation-for="Id" class="text-danger"></span>
        </div>
        <div class="form-group mt-2">
            <input type="submit" value="Consultar ticket" class="btn btn-outline-primary" />
        </div>
    </form>
</div>

